            <!-- å¯¼èˆªæ§åˆ¶ -->
            <div class="navigation">
                <button class="nav-btn" onclick="prevSlide()">â†</button>
                <button class="nav-btn" onclick="nextSlide()">â†’</button>
            </div>

            <!-- é¡µç æŒ‡ç¤ºå™¨ -->
            <div class="page-dots">
                <div class="dot active"></div>
                <div class="dot"></div>
